<div class="main">
    <div class="content">
    <h1>Beers</h1>
    
    <div class="box-cervezas">
        {{#each this.beersFromApi}}
        <div class="cerveza">
            <img class="imagen-cerveza" src="{{this.image_url}}" alt="">
            <h3>Name : {{this.name}}</h3>
            <p>Description : {{this.description}}</p>
            <p>Tagline : {{this.tagline}}</p>
        </div>
        {{/each}}
    </div>

    </div>  
</div>
